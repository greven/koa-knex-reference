{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "node",
      "request": "launch",
      "name": "Run Server",
      "program": "${workspaceFolder}/server/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run cli",
      "program": "${workspaceFolder}/bin/cli",
      "args": [
        "listUsers"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Tests",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "env": { "NODE_ENV": "test" }
    }
  ]
}
